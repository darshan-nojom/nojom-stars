<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/background"
        android:fitsSystemWindows="true">

        <com.google.android.material.appbar.AppBarLayout
            android:id="@+id/appbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent">

            <com.google.android.material.appbar.CollapsingToolbarLayout
                android:id="@+id/collapsing_toolbar"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fitsSystemWindows="true"
                app:contentScrim="@color/white"
                app:layout_collapseMode="parallax"
                app:layout_scrollFlags="scroll|exitUntilCollapsed"
                app:titleEnabled="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_25sdp"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingRight="@dimen/_15sdp"
                    app:layout_collapseMode="parallax">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center">

                        <RelativeLayout
                            android:id="@+id/relative"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true">

                            <de.hdodenhof.circleimageview.CircleImageView
                                android:id="@+id/img_profile"
                                android:layout_width="@dimen/_50sdp"
                                android:layout_height="@dimen/_50sdp"
                                android:src="@mipmap/ic_launcher_round" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignEnd="@+id/img_profile"
                                android:layout_alignBottom="@+id/img_profile"
                                android:layout_marginBottom="@dimen/_4sdp"
                                android:contentDescription="@string/app_name"
                                android:src="@drawable/online_dot"
                                tools:ignore="ImageContrastCheck" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/rel_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/relative"
                            android:layout_centerHorizontal="true"
                            android:gravity="center">

                            <android.textview.TextViewSFTextPro
                                android:id="@+id/tv_name"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/username"
                                android:textAlignment="viewStart"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_14sdp"
                                app:fonts="@string/sfpro_text_midium" />

                            <ImageView
                                android:id="@+id/img_verified"
                                android:layout_width="@dimen/_12sdp"
                                android:layout_height="@dimen/_12sdp"
                                android:layout_centerVertical="true"
                                android:layout_marginStart="@dimen/_7sdp"
                                android:layout_toEndOf="@+id/tv_name"
                                android:contentDescription="@string/app_name"
                                android:src="@drawable/ic_verified"
                                android:visibility="gone" />

                        </RelativeLayout>

                    </RelativeLayout>

                    <android.textview.TextViewSFTextPro
                        android:id="@+id/tv_userName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textColor="@color/tab_gray"
                        android:textSize="@dimen/_12sdp"
                        app:fonts="@string/sfpro_text_regular" />

                    <android.textview.TextViewSFTextPro
                        android:id="@+id/tv_link"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:autoLink="web"
                        android:clickable="true"
                        android:focusable="true"
                        android:textColor="@color/C_44A2E2"
                        android:textSize="@dimen/_11sdp"
                        app:fonts="@string/sfpro_text_regular" />

                    <LinearLayout
                        android:id="@+id/linear_left"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_15sdp"
                        android:orientation="horizontal">

                        <android.textview.TextViewSFTextPro
                            android:id="@+id/tv_sendOffer"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@drawable/white_button_bg"
                            android:gravity="center"
                            android:padding="@dimen/_5sdp"
                            android:text="@string/send_offer"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_12sdp"
                            app:fonts="@string/sfpro_text_regular" />

                        <android.textview.TextViewSFTextPro
                            android:id="@+id/tv_chat"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_10sdp"
                            android:layout_weight="1"
                            android:background="@drawable/white_button_bg"
                            android:gravity="center"
                            android:padding="@dimen/_5sdp"
                            android:text="@string/message"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_12sdp"
                            app:fonts="@string/sfpro_text_regular" />

                    </LinearLayout>

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/rv_platform"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_15sdp"
                        android:orientation="horizontal"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />

                </LinearLayout>

                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/anim_toolbar"
                    android:layout_width="match_parent"
                    android:layout_height="?attr/actionBarSize"
                    android:layout_gravity="top"
                    app:layout_collapseMode="pin"
                    app:popupTheme="@style/ThemeOverlay.AppCompat.Light">

                    <RelativeLayout
                        android:id="@+id/rl_header"
                        android:layout_width="match_parent"
                        android:layout_height="?attr/actionBarSize"
                        android:layout_marginEnd="@dimen/_10sdp">

                        <ImageView
                            android:id="@+id/img_back"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:contentDescription="@string/back"
                            android:src="@drawable/intercom_back"
                            app:tint="@color/black" />

                        <android.textview.TextViewSFTextPro
                            android:id="@+id/toolbar_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:layout_gravity="center"
                            android:ellipsize="end"
                            android:gravity="center"
                            android:maxLength="24"
                            android:singleLine="true"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_14sdp"
                            android:visibility="invisible"
                            app:fonts="@string/sfpro_text_midium" />

                        <ImageView
                            android:id="@+id/img_save"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginEnd="@dimen/_10sdp"
                            android:layout_toStartOf="@+id/img_share"
                            android:contentDescription="@string/back"
                            android:padding="@dimen/_5sdp"
                            android:src="@drawable/ic_fav"
                            app:tint="@color/black" />

                        <ImageView
                            android:id="@+id/img_share"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:layout_centerVertical="true"
                            android:layout_marginEnd="@dimen/_10sdp"
                            android:contentDescription="@string/back"
                            android:src="@drawable/ic_share_new" />
                    </RelativeLayout>


                </androidx.appcompat.widget.Toolbar>
            </com.google.android.material.appbar.CollapsingToolbarLayout>
        </com.google.android.material.appbar.AppBarLayout>

        <androidx.core.widget.NestedScrollView
            android:id="@+id/scroll"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_behavior="@string/appbar_scrolling_view_behavior">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingRight="@dimen/_15sdp"
                    android:paddingBottom="@dimen/_15sdp">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/lin_portfolio"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_25sdp"
                            android:orientation="vertical"
                            android:visibility="gone">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:gravity="center_vertical">

                                <android.textview.TextViewSFTextPro
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentStart="true"
                                    android:ellipsize="end"
                                    android:maxLines="1"
                                    android:text="@string/portfolio"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_14sdp"
                                    app:fonts="@string/sfpro_text_bold" />

                                <RelativeLayout
                                    android:id="@+id/rl_portfolio_view"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentEnd="true"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/white_rounded_corner_20"
                                    android:gravity="center_vertical"
                                    android:paddingLeft="@dimen/_10sdp"
                                    android:paddingTop="@dimen/_3sdp"
                                    android:paddingRight="@dimen/_10sdp"
                                    android:paddingBottom="@dimen/_3sdp">

                                    <android.textview.TextViewSFTextPro
                                        android:id="@+id/viewPortfolioAll"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginBottom="@dimen/_2sdp"
                                        android:text="@string/view_all"
                                        android:textColor="@color/c_b2b2b2"
                                        android:textSize="@dimen/_11sdp"
                                        app:fonts="@string/sfdiaply_regular" />

                                    <ImageView
                                        android:layout_width="@dimen/_8sdp"
                                        android:layout_height="@dimen/_8sdp"
                                        android:layout_centerVertical="true"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:layout_marginTop="@dimen/_3sdp"
                                        android:layout_toEndOf="@id/viewPortfolioAll"
                                        android:contentDescription="@null"
                                        android:gravity="center_vertical"
                                        android:src="@drawable/arrow_next" />
                                </RelativeLayout>
                            </RelativeLayout>

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <androidx.recyclerview.widget.RecyclerView
                                    android:id="@+id/rv_portfolio"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_15sdp"
                                    android:orientation="horizontal"
                                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                                    tools:itemCount="2"
                                    tools:listitem="@layout/item_portfolio_small" />

                                <android.textview.TextViewSFTextPro
                                    android:id="@+id/tv_ph_portfolio"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="@dimen/_20sdp"
                                    android:layout_marginBottom="@dimen/_20sdp"
                                    android:text="@string/no_service"
                                    android:textColor="@color/lightgray"
                                    android:textSize="@dimen/_12sdp"
                                    android:visibility="gone"
                                    app:fonts="@string/sfdiaply_regular" />

                            </FrameLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/lin_services"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_25sdp"
                            android:orientation="vertical"
                            android:visibility="gone">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:gravity="center_vertical">

                                <android.textview.TextViewSFTextPro
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentStart="true"
                                    android:ellipsize="end"
                                    android:maxLines="1"
                                    android:text="@string/services"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_14sdp"
                                    app:fonts="@string/sfpro_text_bold" />

                                <RelativeLayout
                                    android:id="@+id/rel_services_all"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentEnd="true"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/white_rounded_corner_20"
                                    android:gravity="center_vertical"
                                    android:paddingLeft="@dimen/_10sdp"
                                    android:paddingTop="@dimen/_3sdp"
                                    android:paddingRight="@dimen/_10sdp"
                                    android:paddingBottom="@dimen/_3sdp">

                                    <android.textview.TextViewSFTextPro
                                        android:id="@+id/txtServiceAll"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginBottom="@dimen/_2sdp"
                                        android:text="@string/view_all"
                                        android:textColor="@color/c_b2b2b2"
                                        android:textSize="@dimen/_11sdp"
                                        app:fonts="@string/sfdiaply_regular" />

                                    <ImageView
                                        android:layout_width="@dimen/_8sdp"
                                        android:layout_height="@dimen/_8sdp"
                                        android:layout_centerVertical="true"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:layout_marginTop="@dimen/_3sdp"
                                        android:layout_toEndOf="@id/txtServiceAll"
                                        android:contentDescription="@null"
                                        android:gravity="center_vertical"
                                        android:src="@drawable/arrow_next" />
                                </RelativeLayout>
                            </RelativeLayout>

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <androidx.recyclerview.widget.RecyclerView
                                    android:id="@+id/rv_services"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:clipToPadding="false"
                                    android:nestedScrollingEnabled="false"
                                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                                    tools:itemCount="2"
                                    tools:listitem="@layout/item_inf_services" />

                            </FrameLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/lin_store"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_25sdp"
                            android:orientation="vertical"
                            android:visibility="gone">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:gravity="center_vertical">

                                <android.textview.TextViewSFTextPro
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentStart="true"
                                    android:ellipsize="end"
                                    android:maxLines="1"
                                    android:text="@string/store"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_14sdp"
                                    app:fonts="@string/sfpro_text_bold" />

                                <RelativeLayout
                                    android:id="@+id/rl_store_view"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentEnd="true"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/white_rounded_corner_20"
                                    android:gravity="center_vertical"
                                    android:paddingLeft="@dimen/_10sdp"
                                    android:paddingTop="@dimen/_3sdp"
                                    android:paddingRight="@dimen/_10sdp"
                                    android:paddingBottom="@dimen/_3sdp">

                                    <android.textview.TextViewSFTextPro
                                        android:id="@+id/txtStoreAll"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginBottom="@dimen/_2sdp"
                                        android:text="@string/view_all"
                                        android:textColor="@color/c_b2b2b2"
                                        android:textSize="@dimen/_11sdp"
                                        app:fonts="@string/sfdiaply_regular" />

                                    <ImageView
                                        android:layout_width="@dimen/_8sdp"
                                        android:layout_height="@dimen/_8sdp"
                                        android:layout_centerVertical="true"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:layout_marginTop="@dimen/_3sdp"
                                        android:layout_toEndOf="@id/txtStoreAll"
                                        android:contentDescription="@null"
                                        android:gravity="center_vertical"
                                        android:src="@drawable/arrow_next" />
                                </RelativeLayout>
                            </RelativeLayout>

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <androidx.recyclerview.widget.RecyclerView
                                    android:id="@+id/rv_store"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:clipToPadding="false"
                                    android:nestedScrollingEnabled="false"
                                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                                    tools:itemCount="2"
                                    tools:listitem="@layout/item_inf_store" />

                                <android.textview.TextViewSFTextPro
                                    android:id="@+id/tv_ph_store"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="@dimen/_20sdp"
                                    android:layout_marginBottom="@dimen/_20sdp"
                                    android:text="@string/no_service"
                                    android:textColor="@color/lightgray"
                                    android:textSize="@dimen/_12sdp"
                                    android:visibility="gone"
                                    app:fonts="@string/sfdiaply_regular" />

                            </FrameLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/lin_agency"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_20sdp"
                            android:orientation="vertical"
                            android:visibility="gone">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <android.textview.TextViewSFTextPro
                                    android:id="@+id/tv_gig_title"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentStart="true"
                                    android:ellipsize="end"
                                    android:maxLines="1"
                                    android:text="@string/agency"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_14sdp"
                                    app:fonts="@string/sfpro_text_bold" />

                                <RelativeLayout
                                    android:id="@+id/rl_agency_view"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentEnd="true"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/white_rounded_corner_20"
                                    android:gravity="center_vertical"
                                    android:paddingLeft="@dimen/_10sdp"
                                    android:paddingTop="@dimen/_3sdp"
                                    android:paddingRight="@dimen/_10sdp"
                                    android:paddingBottom="@dimen/_3sdp">

                                    <android.textview.TextViewSFTextPro
                                        android:id="@+id/txtAgencyAll"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginBottom="@dimen/_2sdp"
                                        android:text="@string/view_all"
                                        android:textColor="@color/c_b2b2b2"
                                        android:textSize="@dimen/_11sdp"
                                        app:fonts="@string/sfdiaply_regular" />

                                    <ImageView
                                        android:layout_width="@dimen/_8sdp"
                                        android:layout_height="@dimen/_8sdp"
                                        android:layout_centerVertical="true"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:layout_marginTop="@dimen/_3sdp"
                                        android:layout_toEndOf="@id/txtAgencyAll"
                                        android:contentDescription="@null"
                                        android:gravity="center_vertical"
                                        android:src="@drawable/arrow_next" />
                                </RelativeLayout>
                            </RelativeLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:background="@drawable/white_button_bg"
                                android:orientation="vertical"
                                android:padding="@dimen/_15sdp">

                                <android.textview.TextViewSFTextPro
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/agency_name"
                                    android:textAlignment="viewStart"
                                    android:textColor="@color/textgrayAccent"
                                    android:textSize="@dimen/_13sdp"
                                    app:fonts="@string/sftext_regular"
                                    tools:ignore="TextContrastCheck" />

                                <android.textview.TextViewSFTextPro
                                    android:id="@+id/tv_agency_name"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_5sdp"
                                    android:textAlignment="viewStart"
                                    android:textColor="@color/black"
                                    android:textIsSelectable="true"
                                    android:textSize="@dimen/_13sdp"
                                    app:fonts="@string/sftext_medium" />

                                <android.textview.TextViewSFTextPro
                                    android:id="@+id/txt_about"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_15sdp"
                                    android:text="@string/about"
                                    android:textAlignment="viewStart"
                                    android:textColor="@color/textgrayAccent"
                                    android:textSize="@dimen/_13sdp"
                                    android:visibility="gone"
                                    app:fonts="@string/sftext_regular"
                                    tools:ignore="TextContrastCheck" />

                                <android.textview.TextViewSFTextPro
                                    android:id="@+id/tv_agency_about"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_5sdp"
                                    android:textAlignment="viewStart"
                                    android:textColor="@color/black"
                                    android:textIsSelectable="true"
                                    android:textSize="@dimen/_13sdp"
                                    android:visibility="gone"
                                    app:fonts="@string/sftext_medium" />

                                <android.textview.TextViewSFTextPro
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_15sdp"
                                    android:text="@string/contact"
                                    android:textAlignment="viewStart"
                                    android:textColor="@color/textgrayAccent"
                                    android:textSize="@dimen/_13sdp"
                                    app:fonts="@string/sftext_regular"
                                    tools:ignore="TextContrastCheck" />

                                <android.textview.TextViewSFTextPro
                                    android:id="@+id/tv_agency_contact"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_5sdp"
                                    android:textAlignment="viewStart"
                                    android:textColor="@color/black"
                                    android:textIsSelectable="true"
                                    android:textSize="@dimen/_13sdp"
                                    app:fonts="@string/sftext_medium" />

                                <android.textview.TextViewSFTextPro
                                    android:id="@+id/txt_email"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_15sdp"
                                    android:text="@string/email"
                                    android:textAlignment="viewStart"
                                    android:textColor="@color/textgrayAccent"
                                    android:textSize="@dimen/_13sdp"
                                    android:visibility="gone"
                                    app:fonts="@string/sftext_regular"
                                    tools:ignore="TextContrastCheck" />

                                <android.textview.TextViewSFTextPro
                                    android:id="@+id/tv_agency_email"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_5sdp"
                                    android:textAlignment="viewStart"
                                    android:textColor="@color/black"
                                    android:textIsSelectable="true"
                                    android:textSize="@dimen/_13sdp"
                                    android:visibility="gone"
                                    app:fonts="@string/sftext_medium" />


                                <android.textview.TextViewSFTextPro
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
